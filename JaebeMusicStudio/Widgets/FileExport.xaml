        <ProgressBar x:Name="progress" Margin="10,69,10,0" Height="31" VerticalAlignment="Top" HorizontalAlignment="Stretch"/>
        <Button Margin="0,115,10,0" HorizontalAlignment="Right" VerticalAlignment="Top" Width="106" Click="Button_Click">Render</Button>
    </Grid>
</Window>
