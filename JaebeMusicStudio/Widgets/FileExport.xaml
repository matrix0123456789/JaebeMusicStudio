        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JaebeMusicStudio.Widgets"
        mc:Ignorable="d"
        Title="FileExport" Height="343" Width="467">
    <Grid>
        <ComboBox VerticalAlignment="Top" HorizontalAlignment="Left" x:Name="frequency" Margin="10,42,0,0" Width="183">
            <ComboBoxItem>8000</ComboBoxItem>
            <ComboBoxItem>11000</ComboBoxItem>
            <ComboBoxItem>22050</ComboBoxItem>
            <ComboBoxItem>44100</ComboBoxItem>
            <ComboBoxItem>48000</ComboBoxItem>
            <ComboBoxItem>96000</ComboBoxItem>
            <ComboBoxItem>192000</ComboBoxItem>
